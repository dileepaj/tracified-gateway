pipeline {
    agent any
    stages {
        stage('Git Checkout') {
            steps {
                echo 'Checking out from github'

            }
        }
        stage('Build the docker image') {
            steps {
                echo 'Building the docker image..'

            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying....'
                

      }
    }
    }
}